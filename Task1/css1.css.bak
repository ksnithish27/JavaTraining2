.container {
  border: 2px solid black;
margin-top:7%;
margin-bottom:7%;
margin-left:3%;
margin-right:3%;

  padding: 20px;
}

.top-left-text {
  position: absolute;
  top: 50%;
  left: 50%;
}

.top-left-image {
  border: 1px solid black;
  border-radius: 2px;
  padding: 5px;
  width: 80px;
height: 80px;
  position: absolute;
  top: 24%;
  left: 10%;
}

.top-right-image {
  border: 1px solid black;
  border-radius: 2px;
  padding: 5px;
   width: 80px;
height: 80px;
  position: absolute;
  top: 24%;
  right: 10%;
}
.ppimage{
position: absolute;
  top: 50%;
width: 175px;
height:175px;
  right: 10%;
}

